<?xml version="1.0" encoding="utf-8"?>
<elements xmlns="armycalc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="armycalc http://armycalc.com/xmls/twr3elements.xsd" version="3.0">
  
  <element maxCount="1" size="fixed" defaultSize="1">
    
	<uid>runic-sword</uid>
    <name>Runic Sword</name>
    <description>This element can be appended to every hero.</description>
    
    <thumbnail src="media/runic-sword.jpg"></thumbnail>

	<elements maxTotalCount="2">
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-might</uid>
	    <name>Rune Of Might</name>
	  </element>
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-fire</uid>
	    <name>Rune Of Fire</name>
	  </element>
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-ice</uid>
	    <name>Rune Of Ice</name>
	  </element>
	  
	</elements>
  
  </element>
  
  <element maxCount="1" size="fixed" defaultSize="1">
    
	<uid>runic-armor</uid>
    <name>Runic Armor</name>
    <description>This element can be appended to every hero.</description>
    
    <thumbnail src="media/runic-armor.jpg"></thumbnail>

	<elements maxTotalCount="1">
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-protection</uid>
	    <name>Rune Of Protection</name>
	  </element>
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-speed</uid>
	    <name>Rune Of Speed</name>
	  </element>
	  
	</elements>
  
  </element>

  <element maxCount="1" size="fixed" defaultSize="1">
    
	<uid>runic-shield</uid>
    <name>Runic Shield</name>
    <description>This element can be appended to every hero.</description>
    
    <thumbnail src="media/runic-shield.jpg"></thumbnail>

	<elements maxTotalCount="1">
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-stone</uid>
	    <name>Rune Of Stone</name>
	  </element>
	  
	  <element minCount="0" maxCount="1" minSize="1" maxSize="1">
	    <uid>rune-of-invisibility</uid>
	    <name>Rune Of Invisibility</name>
	  </element>
	  
	</elements>
  
  </element>

</elements>
